{
  "name": "node-server-ts",
  "version": "0.0.1",
  "private": "true",
  "description": "Generic boilerplate for Node.js server with TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "prebuild": "rimraf dist",
    "dev": "nodemon",
    "debug": "node --inspect=9229 -r ts-node/register ./src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint './src/**/*.ts'",
    "test": "CI=true jest",
    "test:local": "jest --verbose",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "boilerplate",
    "node",
    "typescript"
  ],
  "author": "victorlcm",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "14.14.20",
    "@types/jest": "26.0.19",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "tslib": "^2.0.3"
  }
}
